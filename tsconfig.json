{
    "compilerOptions": {
      "incremental": true,
      "lib": ["ES2022"],
      "target": "es2019",
      "module": "node16",
      "declaration": true,
      "sourceMap": true,
      "composite": true,
      "strict": true,
      "moduleResolution": "node16",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    }
  }
