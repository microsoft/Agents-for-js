{
  "name": "teams-bots",
  "version": "1.0.0",
  "private": true,
  "description": "Agents SKD teams bots for testing and development",
  "author": "Microsoft",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "npm i",
    "build": "tsc --build",
    "prestart": "npm run build",
    "start": "node --env-file .env ./dist/index.js",
    "prebundle": "npm run build",
    "bundle": "esbuild dist/index.js --bundle --platform=node --metafile=dist/meta.json --outfile=dist/bundle.js --tree-shaking=true --keep-names",
    "docker": "docker build -t botimages.azurecr.io/node-echo-bot:latest ."
  },
  "dependencies": {
    "@microsoft/agents-bot-hosting": "file:../../packages/agents-bot-hosting",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "adaptivecards": "^1.3.1",
    "adaptivecards-templating": "^2.3.1",
    "express": "^5.0.1",
    "express-rate-limit": "^7.5.0"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-types": "^2.40.0",
    "@types/express": "^5.0.0",
    "typescript": "^5.7.3"
  },
  "keywords": []
}
