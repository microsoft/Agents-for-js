    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "empty-agent",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/test-agents/empty-agent/src/index.ts",
            "outFiles": [
                "${workspaceFolder}/**/dist/**/*.js"
            ],
            "preLaunchTask": "build_empty",
            "envFile": "${workspaceFolder}/test-agents/empty-agent/.env",
            "console": "integratedTerminal",
            "internalConsoleOptions": "openOnSessionStart"
        },
